{
  "name": "cmd-ts-too",
  "version": "0.14.1",
  "homepage": "https://cmd-ts.now.sh",
  "license": "MIT",
  "author": "Gal Schlezinger",
  "main": "dist/index.js",
  "files": [
    "dist",
    "batteries"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "npx @biomejs/biome check ./example ./scripts ./src ./test",
    "now-build": "mdbook build --dest-dir=public",
    "start": "npx yarn build --watch",
    "prepublishOnly": "rm -rf dist && npx yarn build && npx yarn test",
    "test": "bun test",
    "ts-node": "./scripts/ts-node",
    "format": "npx @biomejs/biome format --write ./example ./scripts ./src ./test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx yarn lint"
    }
  },
  "sideEffects": false,
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "debug": "^4.3.7",
    "didyoumean": "^1.2.2",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "2.27.9",
    "@swc-node/register": "1.10.9",
    "@types/bun": "^1.1.13",
    "@types/debug": "4.1.12",
    "@types/didyoumean": "1.2.2",
    "@types/fs-extra": "11.0.4",
    "@types/node": "^22.9.0",
    "@types/node-fetch": "2.6.11",
    "@types/request": "2.48.12",
    "@typescript-eslint/eslint-plugin": "8.13.0",
    "@typescript-eslint/parser": "8.13.0",
    "cargo-mdbook": "0.4.4",
    "docs-ts": "0.8.0",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "esm": "3.2.25",
    "execa": "9.5.1",
    "fs-extra": "11.2.0",
    "husky": "9.1.6",
    "infer-types": "0.0.2",
    "node-fetch": "3.3.2",
    "prettier": "3.3.3",
    "request": "2.88.2",
    "tempy": "3.1.0",
    "typedoc": "0.26.11",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "2.1.4",
    "yarn": "^1.22.22"
  }
}
